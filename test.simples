/* Exemplo de um programa escrito em SIMPLES */

programa
inicio
   inteiro a;
   inteiro b <- 5;
   inteiro c;
   inteiro f;
   flutuante d;

   leia a;
   leia c;
   leia f;
   leia d;

   b <- (a + b) * c div c;
   f <- 5 + 2;
   c <- c + f * 5;

   escreva b;
   escreva f;
   escreva c;
   escreva d;

fim

